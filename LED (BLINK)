#define LED_PIN 12

void setup() {
  Serial.begin(9600);
  Serial.print("Program is starting!");
  pinMode(LED_PIN, OUTPUT);
}

void loop() {
  digitalWrite(LED_PIN, HIGH); 
  Serial.println("LED = HIGH");
  delay(1000);
  digitalWrite(LED_PIN, LOW);   
  Serial.println("LED = LOW");
  delay(1000);                      
}
